<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Airlineproject</display-name>
  <welcome-file-list>
    <welcome-file>NewFile.html</welcome-file>

  </welcome-file-list>
   <servlet>
       <servlet-name>loginpage</servlet-name>
          <servlet-class>com.project.loginpage</servlet-class>
            </servlet>
     <servlet>
       <servlet-name>Register</servlet-name>
          <servlet-class>com.project.Register</servlet-class>
            </servlet>
    
    
     <servlet>
       <servlet-name>Foption</servlet-name>
          <servlet-class>com.project.Foptions</servlet-class>
            </servlet>
    
    
      <servlet>
       <servlet-name>payment</servlet-name>
          <servlet-class>com.project.payment</servlet-class>
            </servlet>
    
    
    
   <servlet-mapping>
     <servlet-name>loginpage</servlet-name>
     <url-pattern>/myloginpage</url-pattern>
   </servlet-mapping>

 
  <servlet-mapping>
     <servlet-name>Register</servlet-name>
     <url-pattern>/myRegister</url-pattern>
  </servlet-mapping>
     
     
      <servlet-mapping>
     <servlet-name>Foption</servlet-name>
     <url-pattern>/myFoptions</url-pattern>
  </servlet-mapping>
    
    
    
      <servlet-mapping>
     <servlet-name>payment</servlet-name>
     <url-pattern>/mypayment</url-pattern>
  </servlet-mapping>
    
    
     
</web-app>
